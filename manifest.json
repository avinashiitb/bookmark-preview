{
    "manifest_version": 2,
    "name": "Preview & Bookmark",
    "author": "Aviansh",
    "version": "0.001",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources": [
        "inject-script.js"
    ],
    "permissions": [
        "storage",
        "*://*/*"  
    ],
    "icons": {
        "16": "./icon_16.png",
        "32": "./icon_32.png",
        "48": "./icon_48.png",
        "128": "./icon_128.png"
    },
    "browser_action" : {
        "default_popup": "./popup.html",
        "default_icon": {
            "16": "./icon_16.png",
            "32": "./icon_32.png",
            "48": "./icon_48.png",
            "128": "./icon_128.png"
        }
    }
}